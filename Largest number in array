#include<stdio.h>
int main()
{
int n,i,a[5],large,small;
printf("enter a number of element in array");
scanf("%d",&n);
printf("enter values number in array");
for(i=0;i<n;i++)
scanf("%d",&a[i]);
large=small=a[0];
for(i=0;i<n;i++){
if(a[i]>large)
large=a[i];
}
printf("the largest element in array%d\n",large);
}
